<idea-plugin>
  <id>com.intellij.plugins.eclipsekeymap</id>
  <name>Eclipse Keymap</name>
  <version>1.0.0</version>
  <vendor email="dmitry.matveev@jetbrains.com" url="http://www.jetbrains.com">JetBrains</vendor>

<!--  <change-notes>-->
